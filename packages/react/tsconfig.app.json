{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"lib": ["ESNext", "DOM"],
		"jsx": "react-jsx",
		"moduleResolution": "bundler",
		"strict": true,
		"skipLibCheck": true,

		"declaration": true,
		"declarationDir": "types",
		"emitDeclarationOnly": true,
		"noEmit": false
	},
	"include": ["src"]
}
