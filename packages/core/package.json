{
  "name": "@inkblocks/core",
  "version": "0.1.0",
  "description": "Core utilities and functionality.",
  "type": "module",
  "sideEffects": false,
  "source": "src/index.ts",
  "types": "./types/index.d.ts",
  "main": "./dist/inkblocks.cjs",
  "module": "./dist/inkblocks.js",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/inkblocks.js",
      "require": "./dist/inkblocks.cjs"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css",
      "style": "./dist/style.css"
    }
  },
  "files": [
    "dist",
    "types",
    "src"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "nx vite:preview",
    "lint": "eslint src --max-warnings 0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "typescript": "^5.9.3",
    "vite": "^7.3.0"
  },
  "dependencies": {
    "@lexical/extension": "^0.39.0",
    "@lexical/html": "^0.39.0",
    "@lexical/list": "^0.39.0",
    "@lexical/rich-text": "^0.39.0",
    "lexical": "^0.39.0"
  }
}