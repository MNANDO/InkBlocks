{
  "name": "@inkblocks/core",
  "version": "0.1.0",
  "description": "Core utilities and functionality.",
  "type": "module",
  "sideEffects": false,
  "source": "src/index.ts",
  "types": "./types/index.d.ts",
  "main": "./dist/inkblocks.cjs",
  "module": "./dist/inkblocks.js",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/inkblocks.js",
      "require": "./dist/inkblocks.cjs"
    }
  },
  "files": [
    "dist",
    "types",
    "src"
  ],
  "scripts": {
    "clean": "rm -rf dist types node_modules/.tmp",
    "dev": "vite",
    "build:types": "tsc -p tsconfig.build.json",
    "build": "pnpm run clean && pnpm run build:types && vite build",
    "preview": "vite preview",
    "lint": "eslint src --max-warnings 0"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "vite": "^7.3.0"
  },
  "dependencies": {
    "lexical": "^0.39.0"
  }
}